program IGDWNLDR;

uses
  Vcl.Forms,
  uDownload in 'uDownload.pas' {F_Download};

{$R *.res}

var
   F_Download : TF_Download; 
                              

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TF_Download, F_Download);
  Application.Run;
end.
